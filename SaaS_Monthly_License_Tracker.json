    {
      "parameters": {
        "jsCode": "const reportDate = $('setReportDate').first().json.reportDate;\nconst items = $input.all();\n\n// merge all vendor maps\nconst combined = {};\nitems.forEach(item => { Object.assign(combined, item.json); });\n\n// Pull DocuSign totals from the sub-workflow output (no rewiring needed)\nconst ds = $('Call Docusign Report').first().json || {};\n\nconst report = {\n  'Report Date': reportDate,\n  'Google Workspace Users': combined['Google Workspace'] || 0,\n  'Slack Users': combined['Slack'] || 0,\n  'Microsoft 365 Licenses': combined['Microsoft 365'] || 0,\n  'Notion Users': combined['Notion'] || 0,\n  'ChatGPT Enterprise Users': combined['ChatGPT Enterprise'] || 0,\n  'Mosyle Devices': combined['Mosyle'] || 0,\n  'Apple Business Manager Devices': combined['Apple Business Manager'] || 0,\n  'Action1 Devices': combined['Action1'] || 0,\n  'Fireflies Users': combined['Fireflies'] || 0,\n  'Loom Users': combined['Loom'] || 0,\n\n  // NEW: DocuSign fields\n  'DocuSign Sent (MTD)': ds.sentThisMonth ?? 0,\n  'DocuSign Completed (MTD)': ds.completedThisMonth ?? 0,\n  'DocuSign Sent (YTD)': ds.sentThisYear ?? 0,\n  'DocuSign Completed (YTD)': ds.completedThisYear ?? 0,\n};\n\nreturn [{ json: report }];\n"
      },
